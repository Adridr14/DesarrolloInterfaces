<Window x:Class="Aplicacion01.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Aplicacion01"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Label x:Name="labelPatos" Content="Temporada de Patos" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="304,131,0,0"/>
        <Label x:Name="labelConejos" Content="Temporada de Conejos" HorizontalAlignment="Left" VerticalAlignment="Top"  Margin="304,161,0,0"/>
        <Button x:Name="botonPatos" Content="Temporada de Patos" HorizontalAlignment="Left" Margin="206,217,0,0" VerticalAlignment="Top" Background="PaleGoldenrod" Click="botonPatos_Click"/>
        <Button x:Name="botonConejos" Content="Temporada de Conejos" HorizontalAlignment="Left" Margin="406,217,0,0" VerticalAlignment="Top" Background="PaleTurquoise" Click="botonConejos_Click"/>
    </Grid>
</Window>
